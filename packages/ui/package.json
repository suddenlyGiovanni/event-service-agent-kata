{
	"description": "User interface - SSR/SPA app for interacting with the service call API",
	"devDependencies": {
		"@effect/vitest": "catalog:test",
		"@event-service-agent/typescript": "workspace:*",
		"@total-typescript/ts-reset": "catalog:types",
		"@types/bun": "catalog:types",
		"typescript": "catalog:typescript"
	},
	"name": "@event-service-agent/ui",
	"private": true,
	"scripts": {
		"build": "bun build src/index.ts --outdir ./dist",
		"check": "biome check --write .",
		"dev": "bun --watch src/index.ts",
		"format": "biome format --write .",
		"lint": "biome lint --write .",
		"test": "bun --bun vitest",
		"type-check": "tsc --noEmit"
	},
	"type": "module",
	"version": "0.0.1"
}
